##################################################


DirectoryIndex index.php index.html index2.html


##################################################

#Cutom Rules Begins

#Force www:
RewriteEngine on
RewriteCond %{HTTP_HOST} ^acromedical.com [NC]
RewriteRule ^(.*)$ https://www.acromedical.com/$1 [L,R=301,NC]

#Reemove file extensions from display
RewriteEngine on 
RewriteCond %{REQUEST_FILENAME} !-d 
RewriteCond %{REQUEST_FILENAME}\.php -f 
RewriteRule ^(.*)$ $1.php

RewriteEngine on 
RewriteCond %{REQUEST_FILENAME} !-d 
RewriteCond %{REQUEST_FILENAME}\.html -f 
RewriteRule ^(.*)$ $1.html